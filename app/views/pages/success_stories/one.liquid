---
title: Return a success story
published: true
response_type: application/json
handle: api_success_story
---
{% with_scope _slug: params.slug %}
{% assign success_story = contents.success_stories.first %}
{% endwith_scope %}
{
  "success_story": [
    {
      "prename": {{ success_story.prename | json }},
      "name": {{ success_story.name | json }},
      "place": {{ success_story.place | json }},
      "lost_weight": {{ success_story.lost_weight | json }},
      "start_date": {{ success_story.start_date | json }},
      "programm_duration": {{ success_story.programm_duration | json }},
      "number_of_consultations": {{ success_story.number_of_consultations | json }},
      "feeling_of_abdiction": {{ success_story.feeling_of_abdiction | json }},
      "experience_title": {{ success_story.experience_title | json }},
      "experience": {{ success_story.experience | json }},
      "slug": {{ success_story._slug | json }},
      "before_image": {
        "url": {{ success_story.before_image.url | prepend: host | prepend: "http://" | json }}
      },
      "after_image": {
        "url": {{ success_story.after_image.url | prepend: host | prepend: "http://" | json }}
      }
    }
  ]
}