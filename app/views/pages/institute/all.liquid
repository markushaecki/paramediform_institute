---
title: JSON Alle Institutsbilder
published: true
response_type: application/json
handle: api_institute_pictures
---
{
  "institute_pictures": [
    {% for institute_picture in contents.institute_pictures %}
      {
        "description": {{ institute_picture.description | json }},
        "picture": {
          {% if wagon %}
            "url": {{ institute_picture.picture.url | prepend: host | prepend: "http://" | json }}
          {% else %}
            "url": {{ institute_picture.picture.url | json }}
          {% endif %}
        }
      }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
